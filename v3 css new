  max-width: 100%; /* منع تجاوز عرض الجهاز */
  aspect-ratio: 16 / 9; /* الحفاظ على نسبة العرض إلى الارتفاع */
  border: 1px solid #ddd; /* إطار خفيف */
  border-radius: 8px; /* زوايا ناعمة */
  overflow: hidden; /* منع تجاوز المحتوى الزائد */
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* ظل خفيف */
  margin: 0 auto; /* وسطنة العنصر أفقياً */
  display: flex; /* لتهيئة الوسطنة العمودية (إذا كانت الحاوية أكبر من العنصر) */
  align-items: center; /* وسطنة عمودياً */
  justify-content: center; /* وسطنة أفقياً */
  transform: translateX(3%); /* تحريك العنصر لليمين بنسبة 10% */
}

